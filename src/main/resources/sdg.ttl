sdg:added-notIncluded a mmo:StructuralDefinition ;
	mmo:hasTripleStateA mmo:Added ;
	mmo:hasTripleStateB mmo:NotIncluded ;
	mmo:isConflicting "false"^^xsd:boolean ;
	mmo:automaticResolutionState mmo:Added .


