# Added - X definitions
###########################################################

sdg:added-deleted a mmo:StructuralDefinition ;
	mmo:hasTripleStateA mmo:Added ;
	mmo:hasTripleStateB mmo:Deleted ;
	mmo:isConflicting "true"^^xsd:boolean ;
	mmo:automaticResolutionState mmo:Added .

