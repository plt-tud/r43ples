<fieldset>
	<legend>Merging Report</legend>
	<div id="merge-summary" class="form-horizontal">
   	<div class="form-group" >
    	<label for="graph" class="col-sm-2 control-label">Graph</label>
     	<div class="col-sm-10 " >
       		<input id="graph" class="form-control" type="text" placeholder = {{commit.graphName}} disabled ></input>
     	</div>
   	</div>
   	<div class="form-group" >
    	<label for="strategy" class="col-sm-2 control-label">Strategy</label>
     	<div class="col-sm-10 " >
			<input id="strategy" class="form-control" type="text" placeholder= {{commit.strategy}} disabled></input>
     	</div>
   	</div>
	<div class="form-group" >
		<label class="col-sm-2 control-label">SDD</label>
	    <div class="col-sm-10 ">          
	    	<input type="text" class="form-control" placeholder= {{commit.sddName}} disabled></input>
       	</div>
   	</div>
   	<div class="form-group">
    	<label class="col-sm-2 control-label">Merge</label>
    	<div class="col-sm-4 ">
	    	<input type="text" class="form-control" placeholder= {{commit.branch1}} disabled></input>        
     	</div>
     	<label class="col-sm-2 small-offset-2 control-label">Into</label>
	    <div class="col-sm-4 ">
        	<input type="text" class="form-control" placeholder={{commit.branch2}} disabled></input>         
     	</div>
   	</div>
	<div class="form-group">
    	<label class="col-sm-2 control-label">User</label>
       	<div class="col-sm-10 ">
        	<input type="text" class="form-control" placeholder={{commit.user}} id="user" name="user" disabled></input>
       	</div>
    </div>    
	<div class="form-group">
		<label class="col-sm-2 control-label">Message</label>
	    <div class="col-sm-10 ">
	    	<textarea class="form-control" rows="6" id="message" name="message" placeholder={{commit.message}} disabled></textarea>
	    </div>
	   </div>	  
   	</div>
 </fieldset>